{
    "env_config_base": {
        "obs_elements": [
            "backlog_services_requests",
            "num_services_nodes",
            "num_consolidated",
            "nodes_requests",
            "nodes_usages",
            "nodes_slack"
        ],
        "penalty_one": -1,
        "penalty_two": 0,
        "penalty_three": 0,
        "penalty_four": 10,
        "penalty_five": 10,
        "reward_var_one": 0.75,
        "reward_var_two": 2,
        "reward_var_three": 0,
        "reward_var_four": 0.6,
        "no_action_on_overloaded": true,
        "episode_length": 30,
        "max_services_nodes": 10, 
        "discrete_actions": false,
        "backlog_size": 2,
        "seed": 1204,
        "job_arrival":{
            "mode": "fixed",
            "interval": 5
        },
        "kube":{
            "admin_config": "~/.kube/config",
            "service_image": "sdghafouri/service",
            "namespace": "consolidation",
            "clean_after_exit": true,
            "utilization_image": "sdghafouri/utilization-server"
        }
    },
    "run_or_experiment": "DQN",
    "learn_config": {
        "framework": "torch",
        "train_batch_size": 200,
        "num_gpus": 0.5,
        "model": {
            "fcnet_hiddens": [64, 64],
            "fcnet_activation": "linear",
            "vf_share_layers": true
        },
        "gamma": 0.99,
        "lr": 0.0003,
        "num_workers": 0,
        "observation_filter": "MeanStdFilter",
        "seed": 203
    },
    "stop": {
        "timesteps_total": 2000000
    }
}