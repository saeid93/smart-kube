{
    "env_config_base": {
        "obs_elements": [
            "backlog_services_requests",
            "num_services_nodes",
            "num_consolidated",
            "nodes_requests",
            "nodes_usages",
            "nodes_slack"
        ],
        "penalty_illegal": -1,
        "penalty_u": 1,
        "penalty_c": 1,
        "penalty_v": 1,
        "penalty_g": 1,
        "penalty_p": 1,
        "reward_var_illegal_1": 0.1,
        "reward_var_u_1": 0.1,
        "reward_var_c_1": 0.1,
        "reward_var_v_1": 0.1,
        "reward_var_g_1": 0.1,
        "reward_var_p_1": 0.1,
        "reward_var_illegal_2": 1.05,
        "reward_var_u_2": 1.05,
        "reward_var_c_2": 1.05,
        "reward_var_v_2": 1.05,
        "reward_var_g_2": 1.05,
        "reward_var_p_2": 1.05,
        "reward_option": "proposed",
        "no_action_on_overloaded": true,
        "episode_length": {"grid_search": [10, 20, 50]},
        "max_services_nodes": 10, 
        "discrete_actions": false,
        "backlog_size": {"grid_search": [2, 4, 8]},
        "seed": 1204,
        "target_utilization": [1, 1],
        "job_arrival":{
            "mode": "fixed",
            "interval": 5
        },
        "kube":{
            "admin_config": "~/.kube/config",
            "service_image": "sdghafouri/service",
            "namespace": "consolidation",
            "clean_after_exit": true,
            "utilization_image": "sdghafouri/utilization-server"
        }
    },
    "run_or_experiment": "PPO",
    "learn_config": {
        "sgd_minibatch_size": 128,
        "train_batch_size": 1000,
        "model": {
            "fcnet_hiddens": [64, 64],
            "fcnet_activation": "linear",
            "vf_share_layers": true
        },
        "gamma": 0.99,
        "lr": 0.0003,
        "num_workers": 6,
        "observation_filter": "MeanStdFilter",
        "num_sgd_iter": 1,
        "vf_loss_coeff": 0.01,
        "seed": 203
    },
    "stop": {
        "timesteps_total": 2000000
    }
}
